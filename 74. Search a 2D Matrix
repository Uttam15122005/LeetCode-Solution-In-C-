class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
       int row = matrix.size();
       int cols = matrix[0].size();
       for(int i=0; i<row; i++){
        for(int j=0; j<cols; j++){
            if(target == matrix[i][j]){
                return true;
            }
        }
       }
       return false;
        
    }
};
